{
    "manifest_version": 3,
    "name": "Gradescope++",
    "version": "1.0",
    "description": "Improvements On GradeScope",
    "permissions": ["storage"],
    "action": {
        "default_popup": "html/popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.gradescope.com/*/grade*"],
        "js": ["js/grading.js"]
      }
    ]
  }
  